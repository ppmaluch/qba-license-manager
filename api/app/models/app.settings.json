{
  "kind": "collectionType",
  "collectionName": "apps",
  "info": {
    "name": "App",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "app_id": {
      "type": "string",
      "required": true
    },
    "enabled": {
      "type": "boolean",
      "default": true,
      "required": false
    },
    "owner": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "apps"
    },
    "licenses": {
      "collection": "license",
      "via": "app"
    },
    "app_version": {
      "type": "string"
    },
    "url": {
      "type": "uid",
      "targetField": "app_id"
    }
  }
}
